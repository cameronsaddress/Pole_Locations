# Environment Variables for PoleLocations

# AWS Configuration (for NAIP imagery access)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Google Earth Engine (optional for satellite imagery)
GEE_SERVICE_ACCOUNT=your_service_account@project.iam.gserviceaccount.com
GEE_PRIVATE_KEY_PATH=path/to/private-key.json

# Planet API (optional for commercial satellite)
PLANET_API_KEY=your_planet_api_key_here

# Mapillary (street-level imagery)
MAPILLARY_CLIENT_ID=your_mapillary_client_id
MAPILLARY_CLIENT_SECRET=your_mapillary_client_secret
MAPILLARY_TOKEN=your_mapillary_user_token
MAPILLARY_AUTH_URL=https://www.mapillary.com/connect?client_id=your_mapillary_client_id

# Logging
LOG_LEVEL=INFO

# Development Mode
DEBUG=False
